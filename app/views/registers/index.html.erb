<p id="notice"><%= notice %></p>

<h1>Listing Registers</h1>

<table>
  <thead>
    <tr>
      <th>Fname</th>
      <th>Lname</th>
      <th>Email</th>
      <th>Contactno</th>
      <th>Collegename</th>
      <th>Coladdress</th>
      <th>City</th>
      <th>State</th>
      <th>Pincode</th>
      <th>Fphydirname</th>
      <th>Lphydirname</th>
      <th>Pdcontact</th>
      <th>Sports</th>
      <th>Menwomen</th>
      <th>Singdouble</th>
      <th>Accepted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registers.each do |register| %>
      <tr>
        <td><%= register.fname %></td>
        <td><%= register.lname %></td>
        <td><%= register.email %></td>
        <td><%= register.contactno %></td>
        <td><%= register.collegename %></td>
        <td><%= register.coladdress %></td>
        <td><%= register.city %></td>
        <td><%= register.state %></td>
        <td><%= register.pincode %></td>
        <td><%= register.fphydirname %></td>
        <td><%= register.lphydirname %></td>
        <td><%= register.pdcontact %></td>
        <td><%= register.sports %></td>
        <td><%= register.menwomen %></td>
        <td><%= register.singdouble %></td>
        <td><%= register.accepted %></td>
        <td><%= link_to 'Show', register %></td>
        <td><%= link_to 'Edit', edit_register_path(register) %></td>
        <td><%= link_to 'Destroy', register, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Register', new_register_path %>
